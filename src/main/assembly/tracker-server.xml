<assembly>

	<id>dist</id>
	<formats>
		<format>zip</format>
	</formats>

	<fileSets>

		<fileSet>
			<directory>src/main/conf</directory>
			<outputDirectory>conf</outputDirectory>
			<filtered>true</filtered>
			<includes>
				<include>**/*.*</include>
			</includes>
			<excludes>
				<exclude>tracker.xml</exclude>
			</excludes>
		</fileSet>

	</fileSets>

</assembly>